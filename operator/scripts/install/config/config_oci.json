{
   "environmentName": "myenv",
   "profile": "prod",
   "dns": {
      "type": "oci",
      "oci": {
         "ociConfigSecret": "oci",
         "dnsZoneCompartmentOcid": "dnsZoneCompartmentOcid",
         "dnsZoneOcid": "dnsZoneOcid",
         "dnsZoneName": "my.dns.zone.name"
      }
   },
   "certificates": {
      "issuerType": "acme",
      "acme": {
         "provider": "letsEncrypt",
         "emailAddress": "emailAddress",
         "environment": "production"
      }
   },
   "ingress": {
      "type": "LoadBalancer"
   }
}
apiVersion: addons.cluster.x-k8s.io/v1alpha1
kind: VerrazzanoFleet
metadata:
  name: {{.CLUSTER_ID}}
  namespace: {{.NAMESPACE}}
spec:
  clusterSelector:
    name: verrazzano
  verrazzano:
    spec:
      profile: none
      components:
        certManager:
          enabled: true